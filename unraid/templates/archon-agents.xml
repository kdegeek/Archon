<?xml version="1.0"?>
<Container version="2">
  <Name>Archon-Agents</Name>
  <Repository>ghcr.io/coleam00/archon-agents:latest</Repository>
  <Registry>https://ghcr.io</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/coleam00/Archon/issues</Support>
  <Project>https://github.com/coleam00/Archon</Project>
  <Overview>Archon AI Agents - PydanticAI-based agents service component of the Archon knowledge management system. Handles document processing, embedding generation, and AI-powered operations. Requires Archon Server to be running.</Overview>
  <Category>Productivity: Development: AI:</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/coleam00/Archon/main/unraid/templates/archon-agents.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/coleam00/Archon/main/assets/icon-agents.png</Icon>
  <ExtraParams>--restart=unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText>Support Archon Development</DonateText>
  <DonateLink/>
  <Requires>Archon Server component running. OpenAI API key for AI features.</Requires>
  <Config Name="Agents Port" Target="8052" Default="8052" Mode="tcp" Description="AI agents service port" Type="Port" Display="always" Required="true" Mask="false">8052</Config>
  <Config Name="AppData" Target="/appdata" Default="/mnt/user/appdata/archon/agents" Mode="rw" Description="Agents configuration and cache storage" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/archon/agents</Config>
  <Config Name="Data Storage" Target="/data" Default="/mnt/user/archon-data" Mode="rw" Description="Knowledge base and document storage" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/archon-data</Config>
  <Config Name="Logs" Target="/logs" Default="/mnt/user/appdata/archon/logs/agents" Mode="rw" Description="Agents service log files" Type="Path" Display="advanced" Required="false" Mask="false">/mnt/user/appdata/archon/logs/agents</Config>
  <Config Name="SUPABASE_URL" Target="SUPABASE_URL" Default="" Mode="" Description="Supabase project URL (required) - format: https://xxxxx.supabase.co" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="SUPABASE_SERVICE_KEY" Target="SUPABASE_SERVICE_KEY" Default="" Mode="" Description="Supabase service role key (required) - starts with eyJ..." Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="OPENAI_API_KEY" Target="OPENAI_API_KEY" Default="" Mode="" Description="OpenAI API key for AI features (required for agents)" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="LOG_LEVEL" Target="LOG_LEVEL" Default="INFO" Mode="" Description="Logging level: DEBUG, INFO, WARNING, ERROR" Type="Variable" Display="advanced" Required="false" Mask="false">INFO</Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="User ID for file permissions (99=nobody)" Type="Variable" Display="advanced" Required="false" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Group ID for file permissions (100=users)" Type="Variable" Display="advanced" Required="false" Mask="false">100</Config>
  <Config Name="TZ" Target="TZ" Default="America/New_York" Mode="" Description="Container timezone" Type="Variable" Display="advanced" Required="false" Mask="false">America/New_York</Config>
  <Config Name="EMBEDDING_MODEL" Target="EMBEDDING_MODEL" Default="text-embedding-3-small" Mode="" Description="OpenAI embedding model to use" Type="Variable" Display="expert" Required="false" Mask="false">text-embedding-3-small</Config>
  <Config Name="EMBEDDING_BATCH_SIZE" Target="EMBEDDING_BATCH_SIZE" Default="100" Mode="" Description="Batch size for embedding generation" Type="Variable" Display="expert" Required="false" Mask="false">100</Config>
  <Config Name="AGENT_MAX_WORKERS" Target="AGENT_MAX_WORKERS" Default="4" Mode="" Description="Maximum concurrent agent workers" Type="Variable" Display="expert" Required="false" Mask="false">4</Config>
</Container>